{
  // https://code.visualstudio.com/docs/editor/debugging
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Main Process",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceRoot}",
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
      "program": "${workspaceRoot}/app/main.js",
      //"preLaunchTask": "compile",
      "env": {
        "DEBUG": "vagrant*"
      }
    }
  ]
}
