{
  "name": "vagrant-manager",
  "version": "0.0.1",
  "description": "App to manage vagrant virtual machines",
  "main": "app/main.js",
  "scripts": {
    "compile": "rm -rf app && gulp compile",
    "start": "node_modules/.bin/electron .",
    "start-dev": "npm run compile && DEBUG=vagrant* npm start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "node_modules/.bin/electron-packager ./ VagrantManager --icon=resources/icon.png --out build --ignore=\"(^/build$|^/src$|.idea|.gitignore|gulpfile.js)\""
  },
  "keywords": [
    "vagrant",
    "manager",
    "virtualbox"
  ],
  "author": {
    "name": "Blas Castellano Moreno",
    "email": "b.castellano.moreno@gmail.com",
    "url": "https://github.com/bcastellano"
  },
  "license": "MIT",
  "dependencies": {
    "debug": "^2.2.0",
    "electron-json-storage": "^2.1.0",
    "fix-path": "^2.1.0",
    "react": "^15.3.1",
    "react-bootstrap": "^0.30.3",
    "react-dom": "^15.3.1",
    "shelljs": "^0.7.4"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "devtron": "^1.4.0",
    "electron": "^1.3.5",
    "electron-packager": "^8.2.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2"
  }
}
